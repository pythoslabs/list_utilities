# find elements that are present in both the lists
def intersection(list1, list2): 
    list_intersection = [value for value in list1 if value in list2] 
    return list_intersection

# find elements that are present in list1 , but not in list2
# To find the other way around, swap the arguments
def not_list(list1, list2)
    not_list = [value for value in list1 if value not in list2] 
    return not_list
    
# Remove null value from a list
no_nulls = [x for x in unique_preds if type(x) == str]
